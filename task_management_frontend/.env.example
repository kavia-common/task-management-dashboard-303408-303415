# Frontend configuration (do not commit secrets)

# Base URL of the FastAPI backend. In dev, backend runs on port 3001.
# The app will use REACT_APP_API_BASE first, then REACT_APP_BACKEND_URL, then default to http://localhost:3001
REACT_APP_API_BASE=http://localhost:3001

# Alternative naming (supported)
REACT_APP_BACKEND_URL=http://localhost:3001
